<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homeF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ADB2D8">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="373dp"
            android:text="홈"
            android:textColor="#000000"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ProgressBar
            android:id="@+id/calorieprogressbar"
            style="@style/Widget.AppCompat.ProgressBar.Horizontal"
            android:layout_width="179dp"
            android:layout_height="194dp"
            android:layout_marginStart="116dp"
            android:layout_marginEnd="116dp"
            android:layout_marginBottom="3dp"
            android:indeterminate="false"
            android:longClickable="true"
            android:max="100"
            android:progressDrawable="@drawable/circle_progressbar"
            app:layout_constraintBottom_toTopOf="@+id/textview16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/cal_per_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#FFB55D76"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/calorieprogressbar"
            app:layout_constraintEnd_toEndOf="@+id/calorieprogressbar"
            app:layout_constraintStart_toStartOf="@+id/calorieprogressbar"
            app:layout_constraintTop_toTopOf="@+id/calorieprogressbar"
            tools:text="100%" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="83dp"
            android:layout_marginEnd="89dp"
            android:layout_marginBottom="14dp"
            android:text="현재 섭취한 칼로리 양이에요"
            android:textColor="#000000"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/calorieprogressbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.714"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textview19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="46dp"
            android:layout_marginEnd="47dp"
            android:layout_marginBottom="14dp"
            android:text="현재 운동으로 소모한 칼로리 양이에요"
            android:textColor="#000000"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/exerciseprogressbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ProgressBar
            android:id="@+id/exerciseprogressbar"
            style="@style/Widget.AppCompat.ProgressBar.Horizontal"
            android:layout_width="179dp"
            android:layout_height="194dp"
            android:layout_marginStart="116dp"
            android:layout_marginEnd="116dp"
            android:layout_marginBottom="2dp"
            android:indeterminate="false"
            android:max="100"
            android:progressDrawable="@drawable/circle_progressbar2"
            app:layout_constraintBottom_toTopOf="@+id/textview17"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/exercise_per_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#455DE4"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/exerciseprogressbar"
            app:layout_constraintEnd_toEndOf="@+id/exerciseprogressbar"
            app:layout_constraintStart_toStartOf="@+id/exerciseprogressbar"
            app:layout_constraintTop_toTopOf="@+id/exerciseprogressbar"
            tools:text="0Kcal" />

        <TextView
            android:id="@+id/textview16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="57dp"
            android:layout_marginEnd="57dp"
            android:layout_marginBottom="100dp"
            android:text="원을 꾹 눌러보세요! 더 자세한 정보를 알려드릴게요"
            android:textColor="#FF49454F"
            android:textSize="14dp"
            app:layout_constraintBottom_toTopOf="@+id/textview19"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textview17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="151dp"
            android:layout_marginEnd="145dp"
            android:layout_marginBottom="139dp"
            android:text="원을 다 채워주세요!"
            android:textColor="#FF49454F"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginTop="9dp"
            android:background="#F8F8F8"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12"></LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>